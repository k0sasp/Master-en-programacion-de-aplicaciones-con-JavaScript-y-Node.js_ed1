Ejercicio 1:

var secciones=document.querySelectorAll('.mainWrapper >h2');
var datos=document.querySelectorAll('.mainWrapper >ul >li >a>strong');
var enlaces=document.querySelectorAll('.mainWrapper >ul >li >a')
for (var i = 0; i < datos.length-1; i++) {
  console.log("#"+secciones[i].innerText)
  for (var j = 0; j < datos.length; j++) {
    console.log("*"+datos[j].innerText+"*")
    console.log("["+enlaces[j].href+"]")
}


