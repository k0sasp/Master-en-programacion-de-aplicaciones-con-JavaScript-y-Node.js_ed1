Ejercicio 3

var datos = document.querySelectorAll('.td-module-thumb img');
	
	for (var i = 0; i < datos.length; i++) {
	 console.log(datos[i].src);
	 var ancho = console.log(datos[i].width);
	 var alto = console.log(datos[i].height);
	 datos[i].src = "https://placebear.com/"+ ancho + "/" + alto;
	}
	